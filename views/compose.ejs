<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" type="text" name="postBody" rows="5" cols="80"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
